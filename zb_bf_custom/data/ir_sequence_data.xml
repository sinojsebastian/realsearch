<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

    <!-- Sequence code for res.partner  -->
        <record id="res_partner_sequence_code" model="ir.sequence">
    		<field name="name">Partner Code</field>
    		<field name="code">res.partner</field>
    		<field name="padding">4</field>
    		
		</record> 
		
		
		<!-- Raw Services -->
		 <record forcecreate="1" id="seq_raw_services" model="ir.sequence">
            <field name="name">Raw Services</field>
            <field name="code">raw.services</field>
            <field name="prefix">RS00</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>
        
        <!-- Tenant Partner -->
		<record forcecreate="1" id="sequence_tenant" model="ir.sequence">
            <field name="name">Tenant</field>
            <field name="code">tenant</field>
            <field name="prefix">T</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
